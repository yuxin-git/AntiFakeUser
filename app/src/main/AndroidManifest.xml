<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antifakeuser">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_search"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AntiFakeUser">
        <activity android:name=".ScanActivity"></activity>
        <activity android:name=".ComSearchResultActivity" />
        <activity android:name=".QualifySearchActivity" />
        <activity android:name=".ComSearchActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>